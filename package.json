{
  "name": "brockdev-remix-2eda",
  "private": true,
  "sideEffects": false,
  "type": "module",
  "scripts": {
    "client-env-development": "node ./scripts/writeClientEnv_development.mjs",
    "client-env-production": "node ./scripts/writeClientEnv_production.mjs",
    "update-theme": "node ./theme/themebuilder.mjs",
    "docker-deploy-dev": "npm run docker-deploy-db && npm run docker-deploy-sitemap-generator && npm run docker-deploy-app-dev",
    "docker-deploy": "npm run docker-deploy-db &&  npm run docker-deploy-sitemap-generator && npm run docker-deploy-app",
    "docker-deploy-db": "docker-compose -f ../docker-compose.yml up -d --build postgres",
    "docker-deploy-app": "docker-compose -f ../docker-compose.yml up -d --build --force-recreate bizlocker && docker-compose -f ../docker-compose.yml logs -f bizlocker",
    "docker-deploy-app-dev": "docker-compose -f ../docker-compose.yml up -d --build --force-recreate bizlocker-dev && docker-compose -f ../docker-compose.yml logs -f bizlocker-dev",
    "docker-deploy-sitemap-generator": "docker-compose -f ../docker-compose.yml up -d --build sitemap-generator",
    "dev": "npm run update-theme && npm run client-env-development && npm run update-theme && node ./server.mjs",
    "build": "remix vite:build",
    "start": "rm -rf /var/cache/apt/* && npm install --include=dev && npx prisma migrate deploy && npx prisma generate && npm run client-env-production && npm run update-theme && npm run build && node ./server.mjs && npm run prune-dev-deps",
    "start-dev": "rm -rf /var/cache/apt/* && npm install --include=dev && npx prisma migrate deploy && npx prisma generate && npm run client-env-development && npm run update-theme && npm run build && node ./server.mjs",
    "prune-dev-deps": "npm prune --omit=dev",
    "install-dev-deps": "RUN npm install --include=dev",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint ."
  },
  "prettier": {},
  "eslintIgnore": [
    "/node_modules",
    "/public/build",
    "/build",
    "/public/build",
    "/postgres-data"
  ],
