{
  "name": "brockdev-remix-2eda",
  "private": true,
  "sideEffects": false,
  "scripts": {
    "update-theme": "node ./theme/themebuilder.js",
    "build": "run-s build:*",
    "build:update-theme": "node ./theme/themebuilder.js",
    "build:remix": "remix build",
    "build:server": "esbuild --platform=node --format=cjs ./server.ts --outdir=build --bundle --external:fsevents",
    "dev": "run-p dev:*",
    "dev:server": "cross-env NODE_ENV=development npm run build:server -- --watch",
    "dev:remix": "cross-env remix dev --manual -c \"node --watch-path ./build/server.js --watch ./build/server.js\"",
    "docker": "docker-compose up -d",
    "format": "prettier --write .",
    "lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "setup": "prisma generate && prisma migrate deploy && prisma db seed",
    "start": "cross-env NODE_ENV=production node ./build/server.js",
    "start:mocks": "cross-env NODE_ENV=production node --require --require dotenv/config ./build/server.js",
    "test": "vitest",
    "test:e2e:dev": "start-server-and-test dev http://localhost:3000",
    "pretest:e2e:run": "npm run build",
    "test:e2e:run": "cross-env PORT=8811 start-server-and-test start:mocks http://localhost:8811",
    "typecheck": "tsc && tsc",
    "validate": "run-p \"test -- --run\" lint typecheck test:e2e:run"
  },
  "prettier": {},
  "eslintIgnore": [
    "/node_modules",
    "/build",
    "/public/build",
    "/postgres-data"
  ],
  "dependencies": {
    "@aws-sdk/client-s3": "^3.433.0",
    "@isaacs/express-prometheus-middleware": "^1.2.1",
    "@prisma/client": "^5.7.0",
    "@react-oauth/google": "^0.11.0",
    "@remix-run/css-bundle": "2.3.1",
    "@remix-run/express": "2.3.1",
    "@remix-run/node": "2.3.1",
    "@remix-run/react": "2.3.1",
    "@remix-run/server-runtime": "2.3.1",
    "@sendgrid/mail": "^7.7.0",
    "bcryptjs": "^2.4.3",
    "compression": "^1.7.4",
    "cross-env": "^7.0.3",
    "express": "^4.18.2",
    "get-stream": "^8.0.1",
    "html-react-parser": "^4.0.0",
    "isbot": "^3.6.10",
    "jsonwebtoken": "^9.0.0",
    "leaflet": "^1.9.4",
    "morgan": "^1.10.0",
    "prom-client": "^14.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.10.1",
    "react-leaflet": "^4.2.1",
    "react-quill": "^2.0.0",
    "recharts": "^2.7.2",
    "sanitize-html": "^2.11.0",
    "square": "^28.0.1",
    "sweetalert2": "^11.7.20",
    "swiper": "^11.0.5",
    "tiny-invariant": "^1.3.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^8.0.2",
    "@remix-run/dev": "2.3.1",
    "@remix-run/eslint-config": "2.3.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@types/bcryptjs": "^2.4.2",
    "@types/compression": "^1.7.2",
    "@types/eslint": "^8.40.1",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/leaflet": "^1.9.7",
    "@types/morgan": "^1.9.4",
    "@types/node": "^18.17.1",
    "@types/react": "^18.2.12",
    "@types/react-dom": "^18.2.5",
    "@types/sanitize-html": "^2.9.3",
    "@types/source-map-support": "^0.5.10",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitest/coverage-v8": "^0.32.0",
    "autoprefixer": "^10.4.14",
    "cookie": "^0.5.0",
    "daisyui": "^4.4.19",
    "dotenv": "^16.1.4",
    "esbuild": "^0.18.1",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^8.8.0",
    "happy-dom": "^9.20.3",
    "msw": "^1.2.2",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.24",
    "prettier": "^2.8.8",
    "prettier-plugin-tailwindcss": "^0.3.0",
    "prisma": "^5.7.1",
    "start-server-and-test": "^2.0.0",
    "tailwindcss": "^3.3.2",
    "tailwindcss-bg-patterns": "^0.2.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "vite-tsconfig-paths": "^3.6.0",
    "vitest": "^0.32.0"
  },
  "engines": {
    "node": ">=14"
  },
  "prisma": {
    "seed": "ts-node --require tsconfig-paths/register prisma/seed.ts"
  }
}
